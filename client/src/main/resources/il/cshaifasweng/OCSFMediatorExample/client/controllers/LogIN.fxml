<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane accessibleRole="PASSWORD_FIELD" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.LogIN">
   <children>
      <TextField fx:id="userName" layoutX="200.0" layoutY="156.0" prefHeight="36.0" prefWidth="255.0" promptText="username" style="-fx-background-radius: 15;">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </TextField>
      <Button fx:id="logInBtn" layoutX="269.0" layoutY="273.0" mnemonicParsing="false" onAction="#logIN" prefHeight="36.0" prefWidth="118.0" style="-fx-background-radius: 25;" text="Login">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <TextField fx:id="title" alignment="CENTER" disable="true" layoutX="140.0" layoutY="77.0" style="-fx-background-color: none;" text="Login">
         <font>
            <Font name="Arial Bold" size="30.0" />
         </font>
      </TextField>
      <Label fx:id="notmemberLabel" alignment="CENTER" layoutX="281.0" layoutY="379.0" prefHeight="30.0" prefWidth="93.0" text="not a member?" />
      <Button fx:id="createAcountBtn" alignment="CENTER" layoutX="269.0" layoutY="418.0" mnemonicParsing="false" onAction="#createAcount" prefHeight="19.0" prefWidth="118.0" style="-fx-background-radius: 30;" text="Create account">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="catalogBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#openCatalog" prefHeight="36.0" prefWidth="111.0" style="-fx-background-radius: 15;" text="Catalog" />
      <PasswordField fx:id="Password" accessibleRole="TEXT_FIELD" layoutX="199.0" layoutY="204.0" prefHeight="36.0" prefWidth="255.0" promptText="password" style="-fx-background-radius: 15;">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </PasswordField>
      <TextField fx:id="visiblePassword" disable="true" layoutX="200.0" layoutY="204.0" prefHeight="36.0" prefWidth="255.0" promptText="password" style="-fx-background-radius: 15;" visible="false">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </TextField>
      <CheckBox fx:id="showPassword" layoutX="467.0" layoutY="198.0" mnemonicParsing="false" onAction="#Show" prefHeight="47.0" prefWidth="82.0" text="Show" />
   </children>
</AnchorPane>
