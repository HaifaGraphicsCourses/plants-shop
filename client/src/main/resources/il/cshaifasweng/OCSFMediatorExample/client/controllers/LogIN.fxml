<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="455.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.LogIN">
   <children>
      <TextField fx:id="userName" layoutX="187.0" layoutY="155.0" prefHeight="36.0" prefWidth="255.0" promptText="username" style="-fx-background-radius: 15;">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="Password" layoutX="187.0" layoutY="210.0" prefHeight="36.0" prefWidth="255.0" promptText="password" style="-fx-background-radius: 15;">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </TextField>
      <Button fx:id="logInBtn" layoutX="249.0" layoutY="268.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="118.0" style="-fx-background-radius: 25;" text="Login">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <TextField fx:id="title" alignment="CENTER" layoutX="120.0" layoutY="80.0" style="-fx-background-color: none;" text="Login">
         <font>
            <Font name="Arial Bold" size="30.0" />
         </font>
      </TextField>
      <Label fx:id="notmemberLabel" alignment="CENTER" layoutX="261.0" layoutY="379.0" prefHeight="30.0" prefWidth="93.0" text="not a member?" />
      <Button fx:id="createAcountBtn" alignment="CENTER" layoutX="249.0" layoutY="417.0" mnemonicParsing="false" onAction="#createAcount" prefHeight="19.0" prefWidth="118.0" style="-fx-background-radius: 30;" text="Create account">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="catalogBtn" layoutX="25.0" layoutY="20.0" mnemonicParsing="false" onAction="#openCatalog" prefHeight="36.0" prefWidth="111.0" style="-fx-background-radius: 15;" text="Catalog" />
   </children>
</AnchorPane>
