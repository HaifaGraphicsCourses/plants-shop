<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="819.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.IncomesReportOneTimeInterval">
    <children>
        <LineChart fx:id="reportsChart" layoutX="107.0" layoutY="75.0" prefHeight="449.0" prefWidth="605.0" title="Incomes Report">
            <xAxis>
                <CategoryAxis fx:id="dayAxes" label="Day" side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="incomesNumAxes" label="Incomes" side="LEFT" />
            </yAxis>
        </LineChart>
      <Hyperlink layoutX="772.0" layoutY="14.0" onAction="#backButtonClicked" text="Back" underline="true">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Hyperlink>
      <Button layoutX="344.0" layoutY="525.0" mnemonicParsing="false" onAction="#downloadCSVFile" prefHeight="43.0" prefWidth="131.0" text="Download CSV File" />
    </children>
</AnchorPane>
