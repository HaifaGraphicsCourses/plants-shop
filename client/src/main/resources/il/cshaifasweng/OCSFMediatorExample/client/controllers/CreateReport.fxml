<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.ReportControl">
   <children>
      <Button fx:id="back" contentDisplay="TOP" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Back" textOverrun="CLIP" />
      <Button fx:id="sentBtn" layoutX="500.0" layoutY="14.0" mnemonicParsing="false" onAction="#createRoport" text="Send Report " />
      <TextArea fx:id="reportText" layoutX="60.0" layoutY="110.0" prefHeight="246.0" prefWidth="428.0" />
      <Label layoutX="60.0" layoutY="82.0" prefHeight="27.0" prefWidth="174.0" text="Report Description:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
