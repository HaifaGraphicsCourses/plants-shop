         <font>
            <Font size="15.0" />
         </font></Button>
      <TextArea fx:id="reportText" layoutX="81.0" layoutY="109.0" prefHeight="301.0" prefWidth="477.0" />
      <Label layoutX="81.0" layoutY="74.0" prefHeight="27.0" prefWidth="174.0" text="Report Description:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
