<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.ClientMainPage">
   <children>
      <Button fx:id="catalogBtn" layoutX="240.0" layoutY="80.0" mnemonicParsing="false" onAction="#catalog" prefHeight="60.0" prefWidth="170.0" style="-fx-background-radius: 25;" text="Catalog">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="customizedOrderBtn" layoutX="240.0" layoutY="160.0" mnemonicParsing="false" onAction="#customizedOrder" prefHeight="60.0" prefWidth="170.0" style="-fx-background-radius: 25;" text="Customized Order">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="LogOutBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#logOutBtn" prefHeight="39.0" prefWidth="78.0" style="-fx-background-radius: 25;" text="Log out">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="cartBtn" layoutX="548.0" layoutY="14.0" mnemonicParsing="false" onAction="#Cart" prefHeight="39.0" prefWidth="78.0" style="-fx-background-radius: 25;" text="Cart">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="customizedOrderBtn1" layoutX="240.0" layoutY="240.0" mnemonicParsing="false" onAction="#myOrders" prefHeight="60.0" prefWidth="170.0" style="-fx-background-radius: 25;" text="My Orders ">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <TextField fx:id="messagetextfield" disable="true" layoutX="217.0" layoutY="14.0" prefHeight="57.0" prefWidth="237.0" style="-fx-background-color: none;">
         <font>
            <Font name="Arial" size="20.0" />
         </font>
      </TextField>
      <Button fx:id="viewReportBtn" layoutX="240.0" layoutY="400.0" mnemonicParsing="false" onAction="#viewReport" prefHeight="60.0" prefWidth="170.0" style="-fx-background-radius: 25;" text="View My Reports">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="SendReportBtn" layoutX="240.0" layoutY="320.0" mnemonicParsing="false" onAction="#CreateReport" prefHeight="60.0" prefWidth="170.0" style="-fx-background-radius: 25;" text=" Create Report">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
