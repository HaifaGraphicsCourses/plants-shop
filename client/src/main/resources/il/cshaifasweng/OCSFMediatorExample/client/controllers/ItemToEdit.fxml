<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainPane" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.EditItem">
   <children>
      <AnchorPane fx:id="itemPane" layoutX="14.0" layoutY="64.0" prefHeight="373.0" prefWidth="400.0" />
      <TextField fx:id="saleText" layoutX="430.0" layoutY="278.0" prefHeight="26.0" prefWidth="176.0" />
      <Button fx:id="removeSale" layoutX="542.0" layoutY="367.0" mnemonicParsing="false" onAction="#DelteSale" prefHeight="30.0" prefWidth="75.0" text="Delete Sale" />
      <Button fx:id="editSale" layoutX="542.0" layoutY="322.0" mnemonicParsing="false" onAction="#editSalePercente" prefHeight="30.0" prefWidth="75.0" text="Edit Sale" />
      <Button fx:id="addSale" layoutX="542.0" layoutY="407.0" mnemonicParsing="false" onAction="#addSale" prefHeight="30.0" prefWidth="75.0" text="Add Sale" />
      <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" prefHeight="30.0" prefWidth="75.0" text="Back" />
   </children>
</AnchorPane>
