<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.SignUp">
   <children>
      <TextField fx:id="title" alignment="CENTER" disable="true" layoutX="132.0" layoutY="25.0" style="-fx-background-color: none;" text="Sign Up">
         <font>
            <Font name="Arial Bold" size="30.0" />
         </font>
      </TextField>
      <TextField fx:id="userName" layoutX="192.0" layoutY="119.0" prefHeight="36.0" prefWidth="255.0" promptText="User Name" style="-fx-background-radius: 15;">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </TextField>
      <TextField fx:id="passwordBtn" layoutX="192.0" layoutY="329.0" prefHeight="36.0" prefWidth="255.0" promptText="Password" style="-fx-background-radius: 15;">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="confirmPasswordBtn" layoutX="192.0" layoutY="383.0" prefHeight="36.0" prefWidth="255.0" promptText="Confirm Password" style="-fx-background-radius: 15;">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="IDNumber" layoutX="192.0" layoutY="172.0" prefHeight="36.0" prefWidth="255.0" promptText="ID Number" style="-fx-background-radius: 15;">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </TextField>
      <Button fx:id="SignUpBtn" layoutX="261.0" layoutY="430.0" mnemonicParsing="false" onAction="#signUp" prefHeight="36.0" prefWidth="118.0" style="-fx-background-radius: 25;" text="Sign Up">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#Back" prefHeight="39.0" prefWidth="78.0" style="-fx-background-radius: 25;" text="Back">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <ComboBox layoutX="192.0" layoutY="274.0" prefHeight="36.0" prefWidth="255.0" promptText="Account Type" />
      <TextField fx:id="creditCardNumber" layoutX="192.0" layoutY="222.0" prefHeight="36.0" prefWidth="255.0" promptText="Credit card  number" style="-fx-background-radius: 15;">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </TextField>
      <ComboBox layoutX="462.0" layoutY="274.0" prefHeight="36.0" prefWidth="150.0" promptText="Shops" />
   </children>
</AnchorPane>
