<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.Cart">
   <children>
      <Button fx:id="CheckoutBtn" layoutX="402.0" layoutY="418.0" mnemonicParsing="false" onAction="#Checkout" prefHeight="39.0" prefWidth="172.0" style="-fx-background-radius: 25; -fx-background-color: green;" text="Proceed to Checkout" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#Back" prefHeight="39.0" prefWidth="78.0" style="-fx-background-radius: 25;" text="Back">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="Subtotal" alignment="TOP_LEFT" layoutX="372.0" layoutY="360.0" prefHeight="29.0" prefWidth="254.0" style="-fx-background-color: none;" text="Subtotal:">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <ScrollPane layoutX="33.0" layoutY="149.0" prefHeight="309.0" prefWidth="315.0">
        <content>
          <AnchorPane fx:id="itemscontainer" minHeight="0.0" minWidth="0.0" prefHeight="349.0" prefWidth="318.0" />
        </content>
      </ScrollPane>
      <Label layoutX="33.0" layoutY="89.0" prefHeight="39.0" prefWidth="103.0" text="Items">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Label fx:id="AccountTypeLabel" layoutX="450.0" layoutY="121.0" prefHeight="55.0" prefWidth="97.0" text="Account Type">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="EmptyCartLabel" layoutX="443.0" layoutY="314.0" prefHeight="39.0" prefWidth="89.0" text="EMPTY CART" visible="false">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="stores" disable="true" layoutX="413.0" layoutY="170.0" onAction="#ComboChange" prefWidth="150.0" visible="false" />
   </children>
</AnchorPane>
