<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.Cart">
   <children>
      <TextField fx:id="title" disable="true" layoutX="14.0" layoutY="64.0" style="-fx-background-color: none;" text="items">
         <font>
            <Font size="30.0" />
         </font>
      </TextField>
      <ScrollPane layoutX="25.0" layoutY="130.0" prefHeight="336.0" prefWidth="359.0">
        <content>
          <AnchorPane fx:id="itemsList" minHeight="0.0" minWidth="0.0" prefHeight="343.0" prefWidth="370.0" />
        </content>
      </ScrollPane>
      <Button fx:id="CheckoutBtn" layoutX="434.0" layoutY="426.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="172.0" style="-fx-background-radius: 25;" text="Checkout">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#Back" prefHeight="39.0" prefWidth="78.0" style="-fx-background-radius: 25;" text="Back">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <TextField fx:id="subtotal" alignment="TOP_LEFT" layoutX="433.0" layoutY="378.0" style="-fx-background-color: none;" text="Subtotal:">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
